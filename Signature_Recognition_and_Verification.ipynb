        "        if mode == \"Existing User\":\n",
        "            if not existing_upload.value:\n",
        "                print(\"Upload a test signature.\")\n",
        "                return\n",
        "            test_bytes = list(existing_upload.value.values())[0]['content']\n",
        "            test_path = save_temp_file(test_bytes)\n",
